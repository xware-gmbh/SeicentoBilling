<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template BROWSER_XGA?>
<?import ch.xwr.seicentobilling.entities.Periode?>
<?import com.xdev.ui.XdevVerticalLayout?>
<?import com.xdev.ui.XdevView?>
<?import com.xdev.ui.entitycomponent.table.XdevTable?>
<?import java.lang.String?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="PeriodeView">
    <x:constraints zpos="0" width="100%" height="100%" />
    <XdevVerticalLayout spacing="false" x:name="verticalLayout" margin="">
        <x:constraints zpos="0" width="100%" height="100%" />
        <x:event>attach.attach</x:event>
        <XdevTable x:name="table" x:entity="Periode" x:autoQuery="false">
            <x:entityProperties>
                <entry type="String" property="perName" caption="Periode" />
            </x:entityProperties>
            <x:constraints weighty="1.0" width="100%" height="100%" />
            <x:typeArguments>
                <typeArgument type="Periode" />
            </x:typeArguments>
            <x:event>itemClick.itemClick</x:event>
        </XdevTable>
    </XdevVerticalLayout>
</XdevView>

