<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template BROWSER_XGA?>
<?import ??>
<?import ch.xwr.seicentobilling.entities.RowParameter?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevFieldGroup?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevHorizontalLayout?>
<?import com.xdev.ui.XdevLabel?>
<?import com.xdev.ui.XdevPanel?>
<?import com.xdev.ui.XdevTextField?>
<?import com.xdev.ui.XdevView?>
<?import com.xdev.ui.entitycomponent.combobox.XdevComboBox?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="RowParamPopup">
    <x:constraints zpos="0" width="100%" height="100%" />
    <XdevPanel x:name="panel">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevGridLayout x:name="form">
            <x:constraints zpos="0" width="100%" height="100%" />
            <XdevComboBox tabIndex="24" x:name="cbxValueType">
                <x:constraints gridx="2" gridy="5" weighty="0.0" weightx="1.0" width="0px" height="0px" />
                <x:typeArguments>
                    <typeArgument type="?" />
                </x:typeArguments>
            </XdevComboBox>
            <XdevComboBox tabIndex="25" x:name="comboBoxState">
                <x:constraints gridx="2" gridy="6" weighty="0.0" weightx="1.0" width="0px" height="0px" />
                <x:typeArguments>
                    <typeArgument type="?" />
                </x:typeArguments>
            </XdevComboBox>
            <XdevFieldGroup x:name="fieldGroup" x:entityType="RowParameter">
                <x:typeArguments>
                    <typeArgument type="RowParameter" />
                </x:typeArguments>
                <x:fields>
                    <entry name="cmbRowObject" property="rowObject" />
                    <entry name="txtPrmGroup" property="prmGroup" />
                    <entry name="txtPrmSubGroup" property="prmSubGroup" />
                    <entry name="txtPrmKey" property="prmKey" />
                    <entry name="txtPrmValue" property="prmValue" />
                    <entry name="cbxValueType" property="prmValueType" />
                    <entry name="comboBoxState" property="prmState" />
                    <entry name="txtPrmParamType" property="prmParamType" />
                    <entry name="txtPrmLookupTable" property="prmLookupTable" />
                    <entry name="chkPrmVisible" property="prmVisible" />
                </x:fields>
            </XdevFieldGroup>
            <XdevLabel x:name="lblPrmGroup" value="{$lblPrmGroup.value}">
                <x:constraints gridx="1" gridy="1" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevTextField columns="5" tabIndex="20" x:name="txtPrmGroup">
                <x:constraints gridx="2" gridy="1" weighty="0.0" weightx="1.0" width="100%" height="0px" />
            </XdevTextField>
            <XdevLabel x:name="lblPrmSubGroup" value="{$lblPrmSubGroup.value}">
                <x:constraints gridx="1" gridy="2" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevTextField columns="5" tabIndex="21" x:name="txtPrmSubGroup">
                <x:constraints gridx="2" gridy="2" weighty="0.0" weightx="1.0" width="100%" height="0px" />
            </XdevTextField>
            <XdevLabel x:name="lblPrmKey" value="{$lblPrmKey.value}">
                <x:constraints gridx="1" gridy="3" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevTextField columns="5" tabIndex="22" x:name="txtPrmKey">
                <x:constraints gridx="2" gridy="3" weighty="0.0" weightx="1.0" width="100%" height="0px" />
            </XdevTextField>
            <XdevLabel x:name="lblPrmValue" value="{$lblPrmValue.value}">
                <x:constraints gridx="1" gridy="4" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevTextField columns="5" tabIndex="23" x:name="txtPrmValue">
                <x:constraints gridx="2" gridy="4" weighty="0.0" weightx="1.0" width="100%" height="0px" />
            </XdevTextField>
            <XdevLabel x:name="lblPrmValueType" value="{$lblPrmValueType.value}">
                <x:constraints gridx="1" gridy="5" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevLabel x:name="lblPrmState" value="{$lblPrmState.value}">
                <x:constraints gridx="1" gridy="6" weighty="0.0" weightx="0.0" width="0px" height="0px" />
            </XdevLabel>
            <XdevHorizontalLayout x:name="horizontalLayout" margin="">
                <x:constraints gridx="1" gridy="7" weighty="0.0" weightx="0.0" width="0px" alignment="MIDDLE_CENTER" gridWidth="2" height="0px" />
                <XdevButton icon="FontAwesome.SAVE" caption="{$cmdSave.caption}" tabIndex="12" x:name="cmdSave">
                    <x:constraints weightx="0.0" width="0px" height="0px" />
                    <x:event>click.buttonClick</x:event>
                </XdevButton>
                <XdevButton icon="FontAwesome.CLOSE" caption="{$cmdReset.caption}" tabIndex="11" x:name="cmdReset">
                    <x:constraints weightx="0.0" width="0px" alignment="MIDDLE_RIGHT" height="0px" />
                    <x:event>click.buttonClick</x:event>
                </XdevButton>
            </XdevHorizontalLayout>
        </XdevGridLayout>
    </XdevPanel>
</XdevView>

